{
  "targets": [
    "x86_64-pc-windows-msvc",
    "x86_64-unknown-linux-gnu",
    "x86_64-apple-darwin",
    "aarch64-apple-darwin"
  ],
  "crate": {
    "name": "rust_core",
    "type": "cdylib"
  },
  "output": {
    "dir": ".",
    "name": "index"
  },
  "features": {
    "abi": "node",
    "buildTarget": "release"
  },
  "nodeVersion": {
    "min": "18.0.0"
  },
  "targetEnv": {
    "x86_64-apple-darwin": {
      "MACOSX_DEPLOYMENT_TARGET": "10.12"
    },
    "aarch64-apple-darwin": {
      "MACOSX_DEPLOYMENT_TARGET": "11.0"
    }
  }
}
